{
  "name": "@fanoo2/backend",
  "version": "1.0.0",
  "description": "AI Annotation Backend",
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "tsx index.ts",
    "build": "npx tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/server/index.js",
    "server": "tsx server/index.ts",
    "lint": "eslint . --ext .ts --config .eslintrc.cjs",
    "test": "npm run test:unit && npm run test:health",
    "test:unit": "npx jest",
    "test:health": "npm run build && node -r tsconfig-paths/register dist/index.js --health",
    "test:smoke": "./smoke-test.sh",
    "test:all": "npm run test:unit && npm run test:health && npm run test:smoke",
    "validate-env": "tsx src/env-validation.ts",
    "orchestrate": "tsx orchestrator.ts"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@replit/vite-plugin-cartographer": "^0.2.7",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
    "@types/cors": "^2.8.19",
    "@types/node": "^24.0.10",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.8.2",
    "express": "^4.18.2",
    "livekit-server-sdk": "^2.13.1",
    "nanoid": "^5.1.5",
    "openai": "^4.0.0",
    "pg": "^8.11.0",
    "stripe": "^18.3.0",
    "tsconfig-paths": "^4.2.0",
    "vite": "^6.3.5",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6"
  }
}
